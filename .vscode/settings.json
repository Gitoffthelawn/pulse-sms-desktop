{
    "editor.tabSize": 2,
    "tslint.enable": true,
    "tslint.autoFixOnSave": true,
    "cSpell.words": [
        
    ],
    "cSpell.ignoreWords": [
        
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings",
    ],
    "emmet.excludeLanguages": [
        "markdown",
    ],
    "tslint.alwaysShowStatus": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
    },
    "workbench.colorCustomizations": {
        "editorWarning.foreground": "#f1b900",
        "editorError.foreground": "#ff000080"
    },
}